	var Modal = function () {
	  function Modal() {
	    var lazy = arguments.length <= 0 || arguments[0] === undefined ? false : arguments[0];

	    _classCallCheck(this, Modal);

	    this.lazy = lazy;
	    var that = this;

	    // initially hide all modals
	    $('#campaign .campaign-modal').addClass('modal-hide');

	    var toggleAction = true;
	    $('.js-toggle-modal').on('click touchend', function () {
	      if (toggleAction) {
	        toggleAction = false;
	        var elem = $(this).data('modal');
	        if (elem != undefined) {
	          if ($('#campaign ' + elem).hasClass('modal-show')) {
	            that.closeModal(elem);
	          } else {
	            that.openModal(elem);
	          }
	        }

	        that.calculateFix();

	        setTimeout(function () {
	          toggleAction = true;
	        }, 500);
	      }
	    });

	    var openAction = true;
	    $('.js-open-modal').on('click touchend', function (e) {
	      e.preventDefault();
	      var elem = $(this).data('modal');
	      if (elem != undefined && !$('#campaign ' + elem).hasClass('modal-show')) {
	        openAction = true;
	        if (openAction) {
	          openAction = false;

	          that.openModal(elem);
	          that.calculateFix();

	          setTimeout(function () {
	            openAction = true;
	          }, 500);
	        }
	      }
	    });

	    var closeAction = true;
	    $('.js-close-modal').on('click touchend', function () {
	      if (closeAction) {
	        closeAction = false;

	        var elem = $(this).data('modal');
	        if (elem != undefined && $('#campaign ' + elem).hasClass('modal-show')) {
	          that.closeModal(elem);

	          if (!$(this)[0].hasAttribute('href')) {
	            (0, _menu.scrollTo)('#' + $('#campaign section:first').attr('id'));
	          }

	          that.calculateFix();
	        }

	        setTimeout(function () {
	          closeAction = true;
	        }, 500);
	      }
	    });
	  }

	  _createClass(Modal, [{
	    key: 'closeModal',
	    value: function closeModal(modal) {
	      $('#campaign ' + modal).removeClass('modal-show fade-in').addClass('modal-hide');

	      $('#campaign section:not( ' + modal + ' )').removeClass('modal-hide');
	      setTimeout(function () {
	        $('#campaign section:not( ' + modal + ' )').addClass('fade-in');
	      }, 1);
	    }
	  }, {
	    key: 'openModal',
	    value: function openModal(modal) {
	      // Pause start video
	      (0, _videoPlayer.videoPause)();

	      $('#campaign section:not( ' + modal + ' )').addClass('modal-hide opacity-0');
	      $('#campaign section:not( ' + modal + ' )').removeClass('fade-in');

	      // Fix flicker on menu when activate
	      if ($('#campaign .campaign-menu').length > 0) {
	        $('#campaign .campaign-menu').addClass('modal-fixed');
	        setTimeout(function () {
	          $('#campaign .campaign-menu').removeClass('modal-fixed');
	        }, 600);
	      }

	      $('#campaign ' + modal).removeClass('modal-hide').addClass('modal-show');
	      setTimeout(function () {
	        $('#campaign ' + modal).addClass('fade-in');
	      }, 1);

	      var scrollToPos = hm.campaign.getDevice() === hm.campaign.DEVICE_TABLET && hm.campaign.getEnvironment() === hm.campaign.ENVIRONMENT_JOSH ? 0 : $('#campaign ' + modal).offset().top - $('#campaign .campaign-menu').height();
	      if (hm.campaign.getDevice() === hm.campaign.DEVICE_DESKTOP && hm.campaign.getEnvironment() === hm.campaign.ENVIRONMENT_JOSH) {
	        scrollToPos -= 24;
	      }
	      setTimeout(function () {
	        window.scrollTo(0, scrollToPos);
	      }, 50);

	      // if autoplay-inview video pause it
	      if ($('.autoplay-inview video').length > 0) {
	        $('.autoplay-inview video')[0].pause();
	      }
	    }
	  }, {
	    key: 'calculateFix',
	    value: function calculateFix() {
	      var _this = this;

	      // FF bugfix to lazyload first images
	      setTimeout(function () {
	        window.scrollTo(0, $(window).scrollTop() - 1);
	        $(window).trigger('resize');
	      }, 99);
	      setTimeout(function () {
	        window.scrollTo(0, $(window).scrollTop() + 1);
	      }, 100);

	      if (typeof this.lazy.revalidate == 'function') {
	        this.lazy.revalidate();
	        setTimeout(function () {
	          _this.lazy.revalidate();
	        }, 200);
	      }
	    }
	  }]);

	  return Modal;
	}();

