	  var scollToFunction = function scollToFunction() {

	    if (target.toLowerCase() == '#start') {
	      elem = 0;
	    } else {
	      elem = $(target).offset().top - topMenuHeight - campaignMenuHeight;
	    }

	    menuSectionChange(target.replace('#', ''));

	    $('html, body').stop().animate({
	      scrollTop: elem
	    }, 0);

	    setTimeout(function () {
	      $.each(scrollEvents, function (index, val) {
	        val.enable();
	      });
	    }, 300);
	  };

	  // If clicking menu when modal is visible - set timeout
	  if ($('#campaign .modal-show').length > 0) {
	    setTimeout(function () {
	      scollToFunction();
	    }, 10);
	  } else {
	    scollToFunction();
	  }
	}

