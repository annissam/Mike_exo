/***/ function(module, exports, __webpack_require__) {

	'use strict';

	Object.defineProperty(exports, "__esModule", {
	  value: true
	});

	var _createClass = function () { function defineProperties(target, props) { for (var i = 0; i < props.length; i++) { var descriptor = props[i]; descriptor.enumerable = descriptor.enumerable || false; descriptor.configurable = true; if ("value" in descriptor) descriptor.writable = true; Object.defineProperty(target, descriptor.key, descriptor); } } return function (Constructor, protoProps, staticProps) { if (protoProps) defineProperties(Constructor.prototype, protoProps); if (staticProps) defineProperties(Constructor, staticProps); return Constructor; }; }();

	var _howler = __webpack_require__(28);

	function _classCallCheck(instance, Constructor) { if (!(instance instanceof Constructor)) { throw new TypeError("Cannot call a class as a function"); } }

	var AudioPlayer = function () {
	  function AudioPlayer() {
	    var _this = this;

	    var opts = arguments.length <= 0 || arguments[0] === undefined ? {} : arguments[0];

	    _classCallCheck(this, AudioPlayer);

	    // init howler
	    this.player = new _howler.Howl({
	      src: [opts.src || 'http://hm-framework.mydev.se/environment/build/6lc_spring_fashion/resources/audio/drums.mp3'],
	      loop: true,
	      preload: true,
	      onplay: function onplay() {
	        return _this.handleUI();
	      },
	      onstop: function onstop() {
	        return _this.handleUI();
	      }
	    });
	    $(document).ready(function () {
	      _this.events();
	    });
	  }

	  _createClass(AudioPlayer, [{
	    key: 'events',
	    value: function events() {
	      var _this2 = this;

	      $('.js-audio-player').on('click', function () {
	        if (_this2.player.playing()) {
	          _this2.player.stop();
	        } else {
	          _this2.player.play();
	        }
	      });
	    }
	  }, {
	    key: 'handleUI',
	    value: function handleUI() {
	      if (this.player.playing()) {
	        if (!$('.js-audio-player .eq-bars').hasClass('playing')) {
	          $('.js-audio-player .eq-bars').addClass('playing');
	        }
	      } else {
